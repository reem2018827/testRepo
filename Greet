//javadoc for class
public class Greet {
    public void Morning() {
        System.out.println("Good morning!");
    }
    public void Afternoon() {
        System.out.println("Good afternoon!");
    }
    public void Evening() {
        System.out.println("Good evening!");
    }
    public void Night() {
        System.out.println("Good night!");
    }
    public void Bye() {
        System.out.println("Goodbye!");
    }
    public void Hello() {
        System.out.println("Hello!");
    }
}
